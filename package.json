{
  "name": "@acies/test",
  "version": "2.0.1",
  "description": "Package for testing Acies Core services written in TypeScript",
  "keywords": [],
  "author": "Acies A/S",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AciesDK/ts-test/issues"
  },
  "homepage": "https://github.com/AciesDK/ts-test#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AciesDK/ts-test.git"
  },
  "main": "lib/index.js",
  "module": "lib/index",
  "types": "lib/index.d.ts",
  "type": "module",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "check": "tsc --noEmit --pretty",
    "test": "tap --typecheck --bail --disable-coverage --files test/**/*.test.ts",
    "build": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.828.0",
    "@aws-sdk/client-sns": "^3.828.0",
    "@aws-sdk/lib-dynamodb": "^3.828.0",
    "@aws-sdk/node-http-handler": "^3.370.0",
    "axios": "^1.10.0",
    "chai": "^5.2.0",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/chai": "^5.2.2",
    "@types/node": "^22.15.31",
    "tap": "^21.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}